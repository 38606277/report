			"datatype":"varchar",
			"lookup":"",
			"default":"",
			"isformula":"false"
		},
		{
			"id":"segment5",
			"name":"segment5",
			"datatype":"varchar",
			"lookup":"",
			"default":"",
			"isformula":"false"
		}
	],
	"out":[
		{
			"id":"net_bal",
			"name":"net_bal",
			"datatype":"varchar",
			"link":""
		}
	]
}
-->SELECT
    ifnull(
        sum(
            gb.PERIOD_NET_DR-gb.PERIOD_NET_CR
        ) ,
        0
    ) net_bal
FROM
    gl_bal_${period_year} gb
WHERE 1=1

  
    <if test="period_num!='' ">
   and gb.PERIOD_NUM=${period_num}
</if>
   AND gb.SEGMENT1 like '${segment1}%'
   and gb.segment3 LIKE '${segment3}%'

  
    <if test="segment5!='' ">
   and gb.segment5 LIKE '${segment5}%'
</if>

  </select>

  
