  <select id="aaaa" resultType="BigDecimal" parameterType="Map">

 
 
    <!--{
	"type":"sql",
	"id":"",
	"desc":"aa",
	"db":"jlerp",
	"in":[
		{
			"id":"period_year",
			"name":"period_year",
			"datatype":"varchar",
			"lookup":"",
			"default":"",
			"isformula":"false"
		},
		{
			"id":"period_num",
			"name":"period_num",
			"datatype":"varchar",
			"lookup":"",
			"default":"",
			"isformula":"false"
		},
		{
			"id":"segment1",
			"name":"segment1",
			"datatype":"varchar",
			"lookup":"",
			"default":"",
			"isformula":"false"
		},
		{
			"id":"segment3",
